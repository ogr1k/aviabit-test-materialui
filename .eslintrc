{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": [
    "jsx-a11y"
  ],
  "rules": {
    "semi": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-plusplus": "off",
    "no-mixed-operators": "off",
    "react/prop-types": "off",
    "react/jsx-one-expression-per-line": "off",
    "import/prefer-default-export": "off"
  }
}

